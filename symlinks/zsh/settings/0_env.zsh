export LC_CTYPE="en_US.UTF-8"

export EDITOR="nvim"
export VISUAL="nvim"

export GOPATH="$HOME/.gopath"
export GOBIN="$GOPATH/bin"
export HSPATH="$HOME/.cabal/bin"
export RSPATH="$HOME/.cargo/bin"
export RUBY_PATH="/usr/local/opt/ruby/bin"
export TEXBINDIR="/Library/TeX/texbin"
export GNUBIN="/usr/local/opt/gnu-getopt/bin:/usr/local/opt/coreutils/libexec/gnubin"

export PATH="$GNUBIN:$HOME/.bin:$HOME/.local/bin:$HOME/.rbenv/shims:$RUBY_PATH:$PATH:$HSPATH:$RSPATH:$GOBIN:$TEXBINDIR:/usr/local/opt/gettext/bin"
